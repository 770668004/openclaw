{
  "github": {
    "repo_url": "",
    "branch": "main",
    "commit_message_prefix": "Auto-sync: "
  },
  "sync": {
    "workspace_path": "/home/kousoyu/.openclaw/workspace",
    "include_patterns": [
      "**/*.md",
      "**/*.txt",
      "**/*.json",
      "**/*.js",
      "**/*.py",
      "**/*.sh",
      "memory/**/*",
      "skills/**/*"
    ],
    "exclude_patterns": [
      "**/node_modules/**",
      "**/.git/**",
      "**/*.log",
      "**/temp/**",
      "**/tmp/**"
    ],
    "interval_hours": 20,
    "max_file_size_mb": 10
  },
  "notifications": {
    "enable_success": true,
    "enable_failure": true,
    "channel": "webchat"
  }
}